#include "libft.h"
#include <stddef.h>

void	*ft_memmove(void *dst, const void *src, size_t n)
{
	size_t		count;
	char		*d;
	char		*s;

	d = (char *) dst;
	s = (char *) src;
	count = 0;
	if (d == s)
		return (dst);
	while (count < n)
	{
		d[count] = s[count];
		src++;
		count++;
	}
	return ((void *)d);
}
